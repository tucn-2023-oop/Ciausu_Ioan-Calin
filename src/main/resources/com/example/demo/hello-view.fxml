<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="600" prefWidth="800" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="470.0" prefWidth="760.0" />

    <Label fx:id="welcomeText" />
   <ButtonBar prefHeight="40.0" prefWidth="200.0">
     <buttons>
         <Button fx:id="loginButton" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Log in" />
          <Button fx:id="signupButton" onAction="#onSignupButtonClik" text="Sign up" />
     </buttons>
   </ButtonBar>
</VBox>
